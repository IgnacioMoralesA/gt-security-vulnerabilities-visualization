                "with": {
                  "runs_older_than": "${{ github.event.inputs.runs_older_than || env.SCHEDULED_RUNS_OLDER_THAN }}",
                  "runs_to_keep": "${{ github.event.inputs.runs_to_keep || env.SCHEDULED_RUNS_TO_KEEP }}"
