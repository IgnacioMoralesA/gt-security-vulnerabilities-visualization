      "workflow_json": {
        "name": "release ee cli",
        "true": {
          "release": {
            "branches": [
              "go"
            ],
            "types": [
              "released"
            ]
          }
        },
        "jobs": {
          "binary": {
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "name": "Download Go",
                "uses": "actions/setup-go@v5",
                "with": {
                  "go-version": "1.21.1"
                },
                "id": "go"
              },
              {
                "name": "Check out repository",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Build",
                "run": "echo \"Tag that is going to be used as digger version: ${{ github.event.release.tag_name }}\"\nenv GOOS=linux GOARCH=amd64 CGO_ENABLED=0 go build -ldflags=\"-X digger/pkg/utils.version=${{ github.event.release.tag_name }}\" -o digger ./ee/cli/cmd/digger\n"
              },
              {
                "name": "Publish linux-x64 exec to github",
                "id": "upload-release-asset-linux-x64",
                "uses": "actions/upload-release-asset@v1",
                "env": {
                  "GITHUB_TOKEN": "${{ secrets.GITHUB_TOKEN }}"
                },
                "with": {
                  "upload_url": "${{ github.event.release.upload_url }}",
                  "asset_path": "digger",
                  "asset_name": "digger-ee-cli-Linux-X64",
                  "asset_content_type": "application/octet-stream"
                }
              }
            ]
          }
        }
      }
    },
