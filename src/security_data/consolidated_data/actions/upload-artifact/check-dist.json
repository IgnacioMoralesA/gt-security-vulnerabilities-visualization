          "pull_request": {
            "paths-ignore": [
              "**.md"
            ]
          },
          "workflow_dispatch": null
        },
        "jobs": {
          "check-dist": {
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Setup Node 20",
                "uses": "actions/setup-node@v4",
                "with": {
                  "node-version": "20.x",
                  "cache": "npm"
                }
              },
              {
                "name": "Install dependencies",
                "run": "npm ci"
              },
              {
                "name": "Rebuild the dist/ directory",
                "run": "npm run release"
              },
              {
                "name": "Compare the expected and actual dist/ directories",
                "run": "if [ \"$(git diff --ignore-space-at-eol dist/ | wc -l)\" -gt \"0\" ]; then\n  echo \"Detected uncommitted changes after build.  See status below:\"\n  git diff\n  exit 1\nfi\n",
                "id": "diff"
              },
              {
                "uses": "actions/upload-artifact@v4",
                "if": "${{ failure() && steps.diff.conclusion == 'failure' }}",
                "with": {
                  "name": "dist",
                  "path": "dist/"
                }
              }
            ]
          }
        }
      }
    }
  }
}