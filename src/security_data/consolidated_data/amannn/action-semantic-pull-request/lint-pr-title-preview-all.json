      "workflow_json": {
        "name": "Lint PR title preview (current branch, all options specified)",
        "true": {
          "pull_request": {
            "types": [
              "opened",
              "edited",
              "synchronize"
            ]
          }
        },
        "jobs": {
          "main": {
            "runs-on": "ubuntu-latest",
            "permissions": {
              "pull-requests": "read"
            },
            "steps": [
              {
                "uses": "actions/checkout@v5"
              },
              {
                "uses": "pnpm/action-setup@v4",
                "with": {
                  "version": 9
                }
              },
              {
                "uses": "actions/setup-node@v4",
                "with": {
                  "node-version": 24
                }
              },
              {
                "run": "pnpm install"
              },
              {
                "run": "pnpm build"
              },
              {
                "uses": "./",
                "env": {
                  "GITHUB_TOKEN": "${{ secrets.GITHUB_TOKEN }}"
                },
                "with": {
                  "types": "fix\nfeat\n",
                  "scopes": "core\nui\nJIRA-\\d+\n",
                  "requireScope": false,
                  "disallowScopes": "release\n[A-Z]+\n",
                  "subjectPattern": "^(?![A-Z]).+$",
                  "subjectPatternError": "The subject \"{subject}\" found in the pull request title \"{title}\"\ndidn't match the configured pattern. Please ensure that the subject\ndoesn't start with an uppercase character.\n",
                  "ignoreLabels": "bot\nignore-semantic-pull-request\n",
                  "headerPattern": "^(\\w*)(?:\\(([\\w$.\\-*/ ]*)\\))?: (.*)$",
                  "headerPatternCorrespondence": "type, scope, subject"
                }
              }
            ]
          }
        }
      }
    },
