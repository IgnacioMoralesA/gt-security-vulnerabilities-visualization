      "workflow_json": {
        "name": "e2e Manual Test",
        "true": {
          "workflow_dispatch": {
            "inputs": {
              "ARGS": {
                "description": "ARGS",
                "required": true,
                "default": "-rltgoDzvO --delete --chmod=ugo+rwX --progress"
              },
              "EXCLUDE": {
                "description": "EXCLUDE",
                "required": true,
                "default": "skip_dir/, /node_modules/"
              },
              "SSH_CMD_ARGS": {
                "description": "SSH_CMD_ARGS",
                "required": true,
                "default": "-o StrictHostKeyChecking=no, -o UserKnownHostsFile=/dev/null"
              }
            }
          }
        },
        "env": {
          "TEST_HOST_DOCKER": "./test",
          "TEST_USER": "test"
        },
        "jobs": {
          "e2e-manual": {
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "name": "Checkout",
                "uses": "actions/checkout@v3"
              },
              {
                "name": "E2E Test Specific - Clean up old test files",
                "run": "docker stop ssh-host-container || true && docker rm ssh-host-container || true\n"
              },
              {
                "name": "E2E Test Specific - Create ssh keys",
                "run": "echo $HOME\nls -la $HOME\nssh-keygen -m PEM -t rsa -b 4096 -f \"$HOME/.ssh/id_rsa\" -N \"\"\neval `ssh-agent -s`\nssh-add \"$HOME/.ssh/id_rsa\"\nssh-add -l\necho \"SSH_PRIVATE_KEY<<EOF\" >> $GITHUB_ENV\ncat $HOME/.ssh/id_rsa >> $GITHUB_ENV\necho \"EOF\" >> $GITHUB_ENV\n"
              },
              {
                "name": "E2E Test Specific - Build Host Server Image",
                "working-directory": "${{ env.TEST_HOST_DOCKER }}",
                "run": "docker build \\\n  -t ssh-host-image . \\\n  --build-arg SSH_PUB_KEY=\"$(cat $HOME/.ssh/id_rsa.pub)\"\ndocker run -d -p 8822:22 --name=ssh-host-container ssh-host-image\ndocker exec ssh-host-container sh -c \"hostname --ip-address\" > ip.txt\necho \"REMOTE_HOST<<EOF\" >> $GITHUB_ENV\ncat ip.txt >> $GITHUB_ENV\necho \"EOF\" >> $GITHUB_ENV\n",
                "shell": "bash"
              },
              {
                "name": "E2E Test Specific - Create project file",
                "run": "mkdir test_project2 && cd \"$_\"\ntruncate -s 5MB info2.txt\ncd ../\nmkdir test_project && cd \"$_\"\ntouch index.html\ndate +\"%Y-%m-%d %H:%M:%S,%3N\" >> index.html\ntruncate -s 50MB image.svg\ntruncate -s 5MB info.txt\ntruncate -s 500MB big_file.txt\nmkdir skip_dir && cd \"$_\"\ntruncate -s 5MB text_in_skip_dir.txt\ncd ../\ncat index.html\necho \"test_project:\" && ls -lR\necho \"skip_dir:\" && ls -lR skip_dir\n"
              },
              {
                "name": "e2e Test ssh-deploy action",
                "uses": "easingthemes/ssh-deploy@feature/multi-src",
                "env": {
                  "REMOTE_USER": "${{ env.TEST_USER }}",
                  "ARGS": "${{ github.event.inputs.ARGS }}",
                  "SSH_CMD_ARGS": "${{ github.event.inputs.SSH_CMD_ARGS }}",
                  "SOURCE": "test_project/ test_project2/",
                  "TARGET": "/var/www/html/",
                  "EXCLUDE": "${{ github.event.inputs.EXCLUDE }}",
                  "SCRIPT_BEFORE": "whoami\nls -lR /var/www/html/\n",
                  "SCRIPT_AFTER": "ls -lR /var/www/html/\necho $RSYNC_STDOUT\n"
                }
              }
            ]
          }
        }
      }
    },
