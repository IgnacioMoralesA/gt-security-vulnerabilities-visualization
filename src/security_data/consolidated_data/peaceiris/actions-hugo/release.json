      "workflow_json": {
        "name": "Release",
        "true": {
          "push": {
            "tags": [
              "v*.*.*"
            ]
          }
        },
        "jobs": {
          "release": {
            "runs-on": "ubuntu-18.04",
            "steps": [
              {
                "uses": "actions/checkout@v2"
              },
              {
                "name": "Dump GitHub context",
                "env": {
                  "GITHUB_CONTEXT": "${{ toJson(github) }}"
                },
                "run": "echo \"${GITHUB_CONTEXT}\""
              },
              {
                "name": "Install hub",
                "run": "HUB_VERSION=\"2.13.0\"\nHUB_NAME=\"hub-linux-amd64-${HUB_VERSION}\"\nHUB_TARBALL=\"${HUB_NAME}.tgz\"\nwget -q \"https://github.com/github/hub/releases/download/v${HUB_VERSION}/${HUB_TARBALL}\"\ntar -zxvf \"./${HUB_TARBALL}\"\nmkdir ~/bin\ncp \"./${HUB_NAME}/bin/hub\" ~/bin/\necho \"::add-path::~/bin/\"\nrm -rf \"./${HUB_NAME}\" \"./${HUB_TARBALL}\"\n"
              },
              {
                "name": "Create release",
                "env": {
                  "GITHUB_TOKEN": "${{ secrets.GITHUB_TOKEN }}"
                },
                "run": "TAG_NAME=\"${GITHUB_REF##refs/tags/}\"\necho \"See [CHANGELOG.md](https://github.com/${GITHUB_REPOSITORY}/blob/${TAG_NAME}/CHANGELOG.md) for more details.\" > ./release_notes.md\nsed -i \"1iRelease ${TAG_NAME}\\n\" ./release_notes.md\nhub release create \\\n  --draft \\\n  --prerelease \\\n  --file ./release_notes.md \\\n  \"${TAG_NAME}\"\n"
              }
            ]
          }
        }
      }
    },
