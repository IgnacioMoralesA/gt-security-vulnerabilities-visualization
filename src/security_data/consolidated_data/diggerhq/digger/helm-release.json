        "jobs": {
          "release": {
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "name": "Checkout",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Install Helm",
                "uses": "azure/setup-helm@v4"
              },
              {
                "name": "Login to GitHub Container Registry",
                "run": "echo \"${{ secrets.GITHUB_TOKEN }}\" | helm registry login ghcr.io -u ${{ github.actor }} --password-stdin\n"
              },
              {
                "name": "Package and push chart",
                "run": "cd helm-charts/digger-backend\nhelm package .\nhelm push digger-backend-*.tgz oci://ghcr.io/diggerhq/helm-charts"
              }
            ]
          }
        }
