      "workflow_json": {
        "name": "example-build-artifacts",
        "true": {
          "push": {
            "branches": [
              "master"
            ]
          },
          "pull_request": null,
          "workflow_dispatch": null
        },
        "jobs": {
          "build": {
            "runs-on": "ubuntu-24.04",
            "steps": [
              {
                "name": "Checkout",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Build app",
                "uses": "./",
                "with": {
                  "runTests": false,
                  "build": "npm run build",
                  "working-directory": "examples/nextjs"
                }
              },
              {
                "name": "Store build artifacts",
                "uses": "actions/upload-artifact@v4",
                "with": {
                  "name": "app",
                  "path": "examples/nextjs/build",
                  "if-no-files-found": "error",
                  "retention-days": 1
                }
              }
            ]
          },
          "test": {
            "needs": "build",
            "strategy": {
              "fail-fast": false,
              "matrix": {
                "os": [
                  "ubuntu-24.04",
                  "windows-2025",
                  "macos-15"
                ]
              }
            },
            "runs-on": "${{ matrix.os }}",
            "steps": [
              {
                "name": "Checkout",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Restore build artifacts",
                "uses": "actions/download-artifact@v4",
                "with": {
                  "name": "app",
                  "path": "examples/nextjs/build"
                }
              },
              {
                "name": "Cypress tests",
                "uses": "./",
                "with": {
                  "start": "npm start",
                  "wait-on": "http://localhost:3000",
                  "working-directory": "examples/nextjs"
                }
              }
            ]
          }
        }
