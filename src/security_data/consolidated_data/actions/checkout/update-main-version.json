              {
                "uses": "actions/checkout@v3",
                "with": {
                  "fetch-depth": 0
                }
              },
              {
                "name": "Git config",
                "run": "git config user.name github-actions\ngit config user.email github-actions@github.com\n"
              },
              {
                "name": "Tag new target",
                "run": "git tag -f ${{ github.event.inputs.main_version }} ${{ github.event.inputs.target }}"
              },
              {
                "name": "Push new tag",
                "run": "git push origin ${{ github.event.inputs.main_version }} --force"
              }
            ]
          }
        }
