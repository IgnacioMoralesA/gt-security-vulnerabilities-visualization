              },
              {
                "name": "Rebuild the dist/ directory",
                "run": "npm run build"
              },
              {
                "name": "Compare the expected and actual dist/ directories",
                "run": "if [ \"$(git diff --ignore-space-at-eol dist/ | wc -l)\" -gt \"0\" ]; then\n  echo \"Detected uncommitted changes after build.  See status below:\"\n  git diff\n  exit 1\nfi\n",
                "id": "diff"
              },
              {
                "uses": "actions/upload-artifact@v3",
                "if": "${{ failure() && steps.diff.conclusion == 'failure' }}",
