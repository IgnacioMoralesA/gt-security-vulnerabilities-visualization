          }
        },
        "jobs": {
          "publish": {
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "uses": "actions/checkout@master"
              },
              {
                "name": "Setup env",
                "run": "echo CONTAINER_IMAGE=ghcr.io/${GITHUB_REPOSITORY,,}:${GITHUB_SHA} >> $GITHUB_ENV\necho CONTAINER_IMAGE_LATEST=ghcr.io/${GITHUB_REPOSITORY,,}:latest >> $GITHUB_ENV\n"
              },
              {
                "name": "Build",
                "run": "docker build -t $CONTAINER_IMAGE -t $CONTAINER_IMAGE_LATEST ."
              },
              {
                "name": "Login",
                "run": "docker login -u $GITHUB_ACTOR -p ${{secrets.GITHUB_TOKEN}} ghcr.io"
              },
              {
                "name": "Publish",
                "run": "docker push $CONTAINER_IMAGE\ndocker push $CONTAINER_IMAGE_LATEST\n"
