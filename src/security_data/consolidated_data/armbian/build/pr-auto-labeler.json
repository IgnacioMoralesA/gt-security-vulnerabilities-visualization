      "workflow_json": {
        "name": "Automatic Pull Request Labeling",
        "run-name": "Set labels - PR #${{ github.event.pull_request.number }} (\"${{ github.event.pull_request.title }}\")",
        "true": "pull_request_target",
        "permissions": {
          "contents": "read",
          "pull-requests": "write",
          "issues": "write"
        },
        "jobs": {
          "label-remove": {
            "name": "Remove Ready to merge",
            "if": "${{ github.event.action == 'opened' || github.event.action == 'reopened' || github.event.action == 'synchronize' }}",
            "runs-on": "ubuntu-latest",
            "steps": [
              {
                "name": "Checkout the pull request",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Check for label using GH CLI",
                "id": "check",
                "run": "gh pr view ${{ github.event.pull_request.number }} --json labels -q '.labels[].name' | grep -q 'Ready to merge' && echo \"has_label=true\" >> $GITHUB_OUTPUT || echo \"has_label=false\" >> $GITHUB_OUTPUT\n",
                "env": {
                  "GH_TOKEN": "${{ secrets.GITHUB_TOKEN }}"
                }
              },
              {
                "name": "Remove \"Ready to merge\" label",
                "if": "steps.check.outputs.has_label == 'true'",
                "uses": "PauMAVA/add-remove-label-action@v1.0.3",
                "with": {
                  "github_token": "${{ secrets.GITHUB_TOKEN }}",
                  "add": "",
                  "remove": "Ready to merge"
                }
              }
            ]
          },
          "label-category": {
            "name": "Category Labels",
            "runs-on": "ubuntu-latest",
            "if": "${{ github.event.action == 'opened' || github.event.action == 'reopened' || github.event.action == 'synchronize' }}",
            "steps": [
              {
                "uses": "actions/checkout@v4"
              },
              {
                "uses": "actions/labeler@v5",
                "with": {
                  "repo-token": "${{ secrets.GITHUB_TOKEN }}"
                }
              }
            ]
          },
          "label-size": {
            "name": "Size Label",
            "runs-on": "ubuntu-latest",
            "if": "${{ github.event.action == 'opened' || github.event.action == 'reopened' || github.event.action == 'synchronize' }}",
            "steps": [
              {
                "name": "Label by size",
                "uses": "pascalgn/size-label-action@v0.5.5",
                "env": {
                  "GITHUB_TOKEN": "${{ secrets.GITHUB_TOKEN }}"
                },
                "with": {
                  "sizes": "{\n  \"0\": \"small\",\n  \"50\": \"medium\",\n  \"250\": \"large\"\n}\n"
                }
              }
            ]
          },
          "label-by-date": {
            "name": "Date label (Quarters)",
            "runs-on": "ubuntu-latest",
            "if": "${{ github.event.action == 'opened' || github.event.action == 'reopened' || github.event.action == 'synchronize' }}",
            "steps": [
              {
                "name": "Determine quarter label",
                "env": {
                  "PR_CREATED_AT": "${{ github.event.pull_request.created_at }}"
                },
                "run": "echo \"PR created at: $PR_CREATED_AT\"\n\n# Extract the numeric month (e.g., 04 for April)\nMONTH=$(date -d \"$PR_CREATED_AT\" +%m | sed 's/^0*//')\necho \"Month extracted: $MONTH\"\n\n# Determine quarter-end label based on month\nif [ \"$MONTH\" -le 2 ] || [ \"$MONTH\" -eq 12 ]; then\n  LABEL=\"02\"\nelif [ \"$MONTH\" -le 5 ]; then\n  LABEL=\"05\"\nelif [ \"$MONTH\" -le 8 ]; then\n  LABEL=\"08\"\nelse\n  LABEL=\"11\"\nfi\n\n# Set as environment variable for next step\necho \"LABEL=${LABEL}\" >> $GITHUB_ENV\n"
              },
              {
                "name": "Add quarter label",
                "uses": "PauMAVA/add-remove-label-action@v1.0.3",
                "with": {
                  "github_token": "${{ secrets.GITHUB_TOKEN }}",
                  "add": "${{ env.LABEL }}"
                }
              }
            ]
          }
        }
      }
    },
