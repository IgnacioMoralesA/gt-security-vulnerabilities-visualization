      "workflow_json": {
        "name": "Build OpenWrt Docker image",
        "true": {
          "repository_dispatch": null,
          "workflow_dispatch": {
            "inputs": {
              "source_branch": {
                "description": "Select the source branch",
                "required": false,
                "default": "official",
                "type": "choice",
                "options": [
                  "official",
                  "lede",
                  "immortalwrt"
                ]
              },
              "docker_img": {
                "description": "Set the docker image",
                "required": false,
                "default": "ophub/openwrt-armv8:latest",
                "type": "choice",
                "options": [
                  "ophub/openwrt-armv8:latest",
                  "ophub/openwrt-aarch64:latest"
                ]
              }
            }
          }
        },
        "env": {
          "TZ": "America/New_York",
          "MAKE_DOCKER_SH": "config/docker/make_docker_image.sh",
          "DOCKER_OUTPATH": "out"
        },
        "jobs": {
          "build": {
            "runs-on": "ubuntu-22.04-arm",
            "if": "${{ github.event.repository.owner.id }} == ${{ github.event.sender.id }}",
            "steps": [
              {
                "name": "Checkout",
                "uses": "actions/checkout@v4"
              },
              {
                "name": "Download OpenWrt rootfs file [ ${{ inputs.source_branch }} ]",
                "id": "down",
                "if": "(!cancelled())",
                "run": "# Get OpenWrt file\nopenwrt_url=\"$(curl \\\n                  --header \"authorization: Bearer ${{ secrets.GITHUB_TOKEN }}\" \\\n                  -s \"https://api.github.com/repos/${GITHUB_REPOSITORY}/releases?per_page=100\" |\n                  grep -o \"OpenWrt_.*${{ inputs.source_branch }}.*/.*rootfs.tar.gz\" |\n                  sort -urV |\n                  head -n 1\n              )\"\n[[ -z \"${openwrt_url}\" ]] && echo -e \"OpenWrt file not found!\" && exit 1\n\n[[ -d \"openwrt\" ]] || mkdir -p openwrt\ncurl -fsSL \"https://github.com/${GITHUB_REPOSITORY}/releases/download/${openwrt_url}\" -o \"openwrt/openwrt-armsr-armv8-generic-rootfs.tar.gz\"\n[[ \"${?}\" -ne \"0\" ]] && echo \"OpenWrt file download failed.\" && exit 1\necho \"status=success\" >> ${GITHUB_OUTPUT}\n"
              },
              {
                "name": "Build the Docker image",
                "id": "make",
                "if": "${{ steps.down.outputs.status }} == 'success' && !cancelled()",
                "run": "chmod +x ${MAKE_DOCKER_SH}\n${MAKE_DOCKER_SH}\necho \"status=success\" >> ${GITHUB_OUTPUT}\n"
              },
              {
                "name": "Set up Docker Buildx",
                "id": "buildx",
                "if": "(!cancelled())",
                "uses": "docker/setup-buildx-action@v3"
              },
              {
                "name": "Login to Docker Hub",
                "id": "login",
                "if": "(!cancelled())",
                "uses": "docker/login-action@v3",
                "with": {
                  "username": "${{ secrets.DOCKERHUB_USERNAME }}",
                  "password": "${{ secrets.DOCKERHUB_PASSWORD }}"
                }
              },
              {
                "name": "Push to Docker Hub",
                "id": "push",
                "if": "(!cancelled())",
                "uses": "docker/build-push-action@v5",
                "with": {
                  "push": true,
                  "platforms": "linux/arm64",
                  "context": "${{ env.DOCKER_OUTPATH }}",
                  "tags": "${{ inputs.docker_img }}"
                }
              }
            ]
          }
        }
      }
    },
