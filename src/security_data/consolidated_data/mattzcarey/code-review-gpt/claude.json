              {
                "name": "Run Claude Code",
                "id": "claude",
                "uses": "anthropics/claude-code-action@beta",
                "with": {
                  "anthropic_api_key": "${{ secrets.ANTHROPIC_API_KEY }}",
                  "mcp_config": "{\n  \"mcpServers\": {\n      \"context7\": {\n          \"command\": \"npx\",\n          \"args\": [\"-y\", \"@upstash/context7-mcp\"]\n      }\n  }\n}\n",
                  "allowed_tools": "Bash(bun review),Bash(bun run),Bash(bun test),Bash(bun test:unit),Bash(bun check),Bash(bun check:fix),Bash(bun run build),mcp__context7__resolve-library-id,mcp__context7__get-library-docs",
